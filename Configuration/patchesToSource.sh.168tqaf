cvs co -r V09-19-03-02 PhysicsTools/HepMCCandAlgos
cvs update -r 1.2 TopQuarkAnalysis/TopSkimming/interface/TopDecayChannelFilter.h
cvs update -r 1.6 TopQuarkAnalysis/TopSkimming/interface/TtDecayChannelSelector.h
cvs update -r 1.10 TopQuarkAnalysis/TopSkimming/src/TtDecayChannelSelector.cc
cvs update -r 1.5 TopQuarkAnalysis/TopSkimming/data/TtFullyHadronicFilter.cfi
cvs update -r 1.6 TopQuarkAnalysis/TopSkimming/data/TtFullyLeptonicFilter.cfi
cvs update -r 1.6 TopQuarkAnalysis/TopSkimming/data/TtSemiLeptonicFilter.cfi


#remove the TQAF likelihood ratio and resolution root files
rm TopQuarkAnalysis/*/data/*.root
